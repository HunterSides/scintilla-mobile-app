{
  "expo": {
    "name": "Scintilla",
    "slug": "scintilla-test",
    "owner": "huntersides",
    "version": "1.1.1",
    // "runtimeVersion": "48.0.0",
    "sdkVersion": "48.0.0",
    "runtimeVersion": {
      "policy":"sdkVersion"
    },
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "scintilla-app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "url": "https://u.expo.dev/ddb15756-ad49-4845-8099-37806bd4a23a",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/images/*",
      "assets/fonts/*",
      "assets/icons/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.scintilla"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.scintilla",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ddb15756-ad49-4845-8099-37806bd4a23a"
      }
    }
  }
}
